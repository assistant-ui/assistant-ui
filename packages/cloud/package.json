{
  "name": "assistant-cloud",
  "version": "0.1.17",
  "description": "Cloud integration for assistant-ui",
  "keywords": [
    "assistant",
    "cloud",
    "ai",
    "chat",
    "assistant-ui"
  ],
  "author": "AgentbaseAI Inc.",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "aui-source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./ai-sdk": {
      "aui-source": "./src/ai-sdk/index.ts",
      "types": "./dist/ai-sdk/index.d.ts",
      "default": "./dist/ai-sdk/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "ai-sdk",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "aui-build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "assistant-stream": "^0.3.2"
  },
  "peerDependencies": {
    "@ai-sdk/react": "^2.0.0",
    "@types/react": "*",
    "ai": "^5.0.0",
    "react": "^18 || ^19"
  },
  "peerDependenciesMeta": {
    "@ai-sdk/react": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    },
    "ai": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@ai-sdk/react": "^2.0.118",
    "@assistant-ui/x-buildutils": "workspace:*",
    "@types/node": "^25.2.1",
    "@types/react": "^19.2.7",
    "ai": "^5.0.116",
    "react": "^19.2.3",
    "vitest": "^4.0.18"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "homepage": "https://www.assistant-ui.com/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/assistant-ui/assistant-ui.git",
    "directory": "packages/cloud"
  },
  "bugs": {
    "url": "https://github.com/assistant-ui/assistant-ui/issues"
  }
}
